{
  "name": "IndieKator",
  "description": "Das beste Festival von hier bis Westberlin.",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --modern --mode 'development'",
    "build:prod": "vue-cli-service build --modern",
    "deploy:dev": "node scripts/deploy.js dev",
    "deploy:prod": "node scripts/deploy.js prod",
    "tVersion": "node scripts/version.js",
    "test:e2e-local": "vue-cli-service test:e2e --config ./nightwatch.conf.json --env default",
    "test:e2e-remote": "vue-cli-service test:e2e --config ./nightwatch.conf.json --env integration --url"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-carousel": "^0.17.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-standard": "^3.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.0.1",
    "shelljs": "^0.8.2",
    "vue-template-compiler": "^2.5.17"
  }
}
